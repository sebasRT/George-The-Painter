{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/free-estimate-request\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/contact-form\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/drywall-repair\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/exterior-painting\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/interior-painting\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/power-washing\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/robots\\.txt\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}