---
import Header from "../global/Header.astro";
import Footer from "../global/Footer.astro";
import BaseHead from "../global/BaseHead.astro";

interface Props {
    title: string;
}
const { title } = Astro.props;
---

<html lang="en" class="bg-white">
    <BaseHead title={title} description="Interior & exterior painting services in Morristown NJ (New Jersey) and surroundings."/>
    <body class="text-gray-950">
        <Header />
        <slot />
        <Footer/>
    </body>
</html>
<style>
    body{
        font-family: "Inter", sans-serif
    }
</style>
